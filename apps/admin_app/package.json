    "@types/insert-text-at-cursor": "^0.3.0",
    "@types/linkifyjs": "^2.1.3",
    "@types/lodash-es": "^4.17.3",
    "@types/recordrtc": "^5.6.1",
    "@types/webappsec-credential-management": "^0.5.1",
    "@types/chart.js": "^2.9.23",
    "@types/core-js": "^2.5.3",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "es-dev-commonjs-transformer": "^0.2.0",
    "concurrently": "^5.1.0",
    "deepmerge": "^3.2.0",
    "es-dev-server": "^1.56.1",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.11.0",
    "husky": "^1.0.0",
    "koa-proxies": "^0.11.0",
    "lint-staged": "^8.0.0",
    "prettier": "^2.0.4",
    "rimraf": "^2.6.3",
    "rollup": "^2.21.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "ts-lit-plugin": "^1.2.0",
    "tslib": "^1.11.0",
    "typescript": "~3.8.2"
  },
  "eslintConfig": {
    "extends": [
      "@open-wc/eslint-config",
      "eslint-config-prettier"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "arrowParens": "avoid"
  },
  "name": "admin-app",
